{
  "name": "audfprint-gui",
  "author": "mitin001",
  "description": "GUI for audfprint",
  "version": "0.1.2",
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.0",
    "@mui/material": "^5.5.0",
    "about-window": "^1.14.0",
    "dotenv": "^8.2.0",
    "electron-log": "^4.3.4",
    "electron-updater": "^4.3.8",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "glob": "^7.2.0",
    "python-shell": "^3.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^16.13.1",
    "react-tooltip": "^4.2.6",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-plugin-serve": "^1.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start_local": "ESLINT_NO_DEV_ERRORS=true npm start",
    "build": "react-scripts build && cp -r electron build/electron",
    "test": "react-scripts test",
    "lint": "./node_modules/.bin/eslint . --fix",
    "prettier": "prettier --config .prettierrc.json --write 'src/**/*{css,js,jsx,json}'",
    "start-electron": "electron .",
    "package-electron-mac": "electron-builder build --mac -c.extraMetadata.main=build/electron/main.js --publish always",
    "package-electron-win": "electron-builder build --win -c.extraMetadata.main=build/electron/main.js"
  },
  "main": "electron/main.js",
  "build": {
    "appId": "github.com/mitin001/audfprint-gui",
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "icon": "build/icon.png"
    },
    "nsis": {
      "artifactName": "${productName}.setup.${version}.${ext}",
      "uninstallDisplayName": "audfprint-gui"
    },
    "productName": "audfprint-gui",
    "publish": {
      "owner": "mitin001",
      "provider": "github",
      "repo": "audfprint-gui"
    },
    "win": {
      "icon": "build/icon.png"
    },
    "asarUnpack": [
      "build/audfprint/**/*"
    ]
  },
  "homepage": "./",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.10.5",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "path-browserify": "^1.0.1",
    "prettier": "2.0.5",
    "serve": "^11.3.2"
  }
}
